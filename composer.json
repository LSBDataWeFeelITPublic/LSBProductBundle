{
    "name": "lsb/product-bundle",
    "type": "symfony-bundle",
    "description": "LSB product catalogue",
    "license": "MIT",
    "keywords": [
        "symfony"
    ],
    "homepage": "https://lsb.pl",
    "authors": [
        {
            "name": "LSB DATA",
            "email": "krzysztof.mazur@lsb.com.pl"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "symfony/framework-bundle": "^5.0",
        "doctrine/orm": "2.*",
        "doctrine/doctrine-bundle": "2.*",
        "symfony/validator": "^5.0",
        "lsb/utility-bundle": "dev-master",
        "knplabs/doctrine-behaviors": "^2.0",
        "stof/doctrine-extensions-bundle": "^1.4",
        "a2lix/translation-form-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/phpunit-bridge": "^5.0",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "LSB\\ProductBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/LSBDataWeFeelITPublic/TranslationFormBundle.git"
        },
        {
            "type": "git",
            "url": "https://github.com/LSBDataWeFeelITPublic/LSBUtilityBundle.git"
        }
    ]
}
